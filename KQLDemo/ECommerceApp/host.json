{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": false, // Disable locally to see EVERY log
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    },
    "logLevel": {
      "default": "Information",
      "Function": "Information",
      "Microsoft": "Warning" // Hides noisy internal Azure logs
    }
  }
}